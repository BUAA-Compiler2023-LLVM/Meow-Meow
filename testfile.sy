const int maxN = 1000;
const int space = 32;
int array[maxN];

void findSmallest(int low, int high, int k, int n) {
  if (low == high)
    return;
  else {
      findSmallest(1, high, k, n);
  }
}

int main() {
  int n = getint(), k = getint();
  int low = 0, high = n - 1;
  findSmallest(low, high, k, n);
  return 0;
}